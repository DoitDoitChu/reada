{
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": ["airbnb-base", "prettier"],
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": "off",
    "func-names": "off",
    "no-shadow": "off"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  }
}
